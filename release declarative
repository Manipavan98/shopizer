pipeline {
    agent {label 'jdk11'}
       triggers {
        cron('30 5 * * *')
   }   
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/Manipavan98/shopizer.git' , branch : 'develop' 
            }
        }
        stage('install') { 
            steps {
                sh 'mvn package'
            }
        }
    }
}