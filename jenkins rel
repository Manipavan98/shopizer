pipeline {
    agent {label 'jdk11'}   
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/Manipavan98/shopizer.git' , branch : 'release' 
            }
        }
        stage ('merge') {
            steps {
                sh 'git merge --no-ff develop'
            }
        }
        stage('install') { 
            steps {
                sh 'mvn package'
            }
        }
    }
}